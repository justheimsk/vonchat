{
  "name": "vonchat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "tsc -b && vite build",
    "lint": "biome check ./src/",
    "lint:staged": "biome check ./src/ --staged",
    "lint:write": "biome check ./src/ --write",
    "preview": "vite preview --host --port 3000",
    "prepare": "cd .. && husky install client/.husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "sass-embedded": "^1.81.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
